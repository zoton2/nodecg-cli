    "chalk": "^2.0.0",
    "clone": "^2.1.0",
    "commander": "^2.6.0",
    "digitalocean-v2": "0.0.2",
    "escape-string-regexp": "^1.0.5",
    "fs-promise": "^2.0.0",
    "github": "^9.0.0",
    "hosted-git-info": "^2.2.0",
    "inquirer": "^4.0.0",
    "js-yaml": "^3.8.1",
    "json-schema-defaults": "^0.3.0",
    "keypair": "^1.0.1",
    "node-forge": "^0.7.0",
    "node-ssh": "^4.0.1",
    "npm-package-arg": "^4.1.0",
    "ora": "^1.1.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
